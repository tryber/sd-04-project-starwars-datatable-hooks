{
  "extends": "airbnb",
  "parser": "babel-eslint",
  "rules": {
    "arrow-parens": [2, "always"],
    "func-names": "off",
    "jsx-a11y/label-has-associated-control": 0,
    "no-alert": "off",
    "no-console": "off",
    "no-shadow": "off",
    "prefer-arrow-callback": "off",
    "react/jsx-filename-extension": [
      1,
      {
        "extensions": [".js", ".jsx"]
      }
    ],
    "react/jsx-fragments": "off",
    "react/prefer-stateless-function": 0
  },
  "env": {
    "browser": true,
    "node": true,
    "jest": true
  }
}
